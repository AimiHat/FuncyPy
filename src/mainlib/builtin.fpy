# List Concatenation
def concat list1 list2 =
	if (isempty list2):
		list1
	else:
		concat (append list1 (head list2)) (tail list2)


# Map function
def map f list =
	if (isempty list):
		[]
	else:
		concat ([f (head list)]) (map f (tail list))

